{
    "name": "roneichiarandi/skeleton-php",
    "description": "A simple Skeleton to create project or library with PHP.",
    "keywords": [ "skeleton", "php", "start" ],
    "require": {
        "php": ">=5.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ronei Chiarandi",
            "email": "ronei.chiarandi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MyPhp\\Skeleton\\": "src"
        }
    },
    "scripts" : {
        "post-create-project-cmd": "./skeleton",
        "test" : "phpunit"
   },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
