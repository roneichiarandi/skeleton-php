{
    "name": "roneichiarandi/skeleton-php",
    "description": "A simple Skeleton to build your project or library with PHP.",
    "type": "skeleton, php, mvc, start, configuration",
    "require": {
        "php": ">=5.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ronei Chiarandi",
            "email": "ronei.chiarandi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MyPhp\\Skeleton\\": "src"
        }
    },
    "scripts" : {
        "test" : "phpunit",
        "phpmd" : "phpmd ./src text cleancode, codesize, controversial, design, naming, unusedcode",
        "phpcs" : "phpcs ./src"
   },
    "minimum-stability": "stable"
}
